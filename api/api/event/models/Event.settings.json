{
  "connection": "default",
  "collectionName": "events",
  "info": {
    "name": "event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "author": {
      "model": "user",
      "via": "events",
      "plugin": "users-permissions"
    },
    "details": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "slug": {
      "type": "string"
    },
    "information": {
      "group": "information",
      "type": "group"
    },
    "friends": {
      "collection": "user",
      "via": "joinedEvents",
      "dominant": true,
      "plugin": "users-permissions"
    }
  }
}